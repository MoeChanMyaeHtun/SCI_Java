package com.sci;

public class LoopingDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		whileLoopDemo();
		doWhileLoop() ;
	}
	private static void whileLoopDemo() {
		int i = 0;
				while (i<10) {
					System.out.println(i);
					i++; //break point to stop loop.
				}
	}
	public static void doWhileLoop() {
		int i = 10;
		do {
			System.out.println(i + "");
			i++;
		} while (i < 10);
	}

}
