package com.sci;

public class HomeWork {

	public static void main(String[] args) {
//		loopZero();
		System.out.println();
		loopOne();
	}
//	0
//	00
//	000
//	0000
//	00000
	private static void loopZero() {
		for(int i = 0; i < 5; i++) {
			for (int j = 0; j <= i; j++) {
				System.out.print("0");
			}
			System.out.println();
		}
	}
	
	private static void loopOne() {
		 for(int i = 0; i <= 5; i++) {
			 System.out.println(i);
		        for (int j = 1; j <= i; j++) {
		            System.out.print(j);
		        }
		        System.out.println();
		    }
	}
	
}
